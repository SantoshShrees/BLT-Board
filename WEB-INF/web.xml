<?xml version="1.0" encoding="UTF-8"?>
<web-app>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateAccountServlet</servlet-name>
        <servlet-class>servlet.CreateAccountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateAccountServlet</servlet-name>
        <url-pattern>/CreateAccountServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CreateThreadServlet</servlet-name>
        <servlet-class>servlet.CreateThreadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateThreadServlet</servlet-name>
        <url-pattern>/CreateThreadServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ThreadServlet</servlet-name>
        <servlet-class>servlet.ThreadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ThreadServlet</servlet-name>
        <url-pattern>/ThreadServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PostServlet</servlet-name>
        <servlet-class>servlet.PostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PostServlet</servlet-name>
        <url-pattern>/PostServlet</url-pattern>
    </servlet-mapping>
</web-app>
